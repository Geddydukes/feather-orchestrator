{
  "title": "Feather Agent Observability",
  "tags": ["feather", "agent"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "timeseries",
      "title": "Run Duration (p95)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(demo_agent_run_duration_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Tool Latency",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "refId": "B",
          "expr": "rate(demo_agent_tool_duration_sum[5m]) / rate(demo_agent_tool_duration_count[5m])",
          "legendFormat": "avg"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Cache Hit Rate",
      "gridPos": { "x": 0, "y": 8, "w": 8, "h": 6 },
      "targets": [
        {
          "refId": "C",
          "expr": "sum(increase(demo_agent_tool_cache_hits_total[5m])) / sum(increase(demo_agent_tool_calls_total[5m]))",
          "legendFormat": "hit rate"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" } }
    },
    {
      "type": "stat",
      "title": "Token Spend (5m)",
      "gridPos": { "x": 8, "y": 8, "w": 8, "h": 6 },
      "targets": [
        {
          "refId": "D",
          "expr": "sum(increase(demo_agent_tokens_total[5m]))",
          "legendFormat": "tokens"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" } }
    },
    {
      "type": "stat",
      "title": "Step Errors (5m)",
      "gridPos": { "x": 16, "y": 8, "w": 8, "h": 6 },
      "targets": [
        {
          "refId": "E",
          "expr": "sum(increase(demo_agent_step_errors_total[5m]))",
          "legendFormat": "errors"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" } }
    }
  ]
}
